#include <stdio.h>
#include <stdlib.h>

int main(void) 
{
    int n[5], soma;
    int i;
    int notaFinal; 
    soma = 0;


    for (i = 0; i < 5; i++)
    {
        printf("\n Digite um numero %d : ", i);
        scanf("%d", &n[i]);

        while (n[i] < 0  n[i] > 20)
        {
            printf(" Nota invalida! Digite novamente: ");
            scanf("%d", &n[i]);
        }
    }


    for (i = 0; i < 5; i++)
    {
        soma = soma + n[i];
    }


    if (soma >= 70)
    {
        printf("\n Aprovado direto! Nota = %d", soma);
    }

    else if (soma >= 50 && soma <= 69)
    {
 
        printf("\n Voce esta na FINAL! nota = %d", soma);


        printf("\n Digite a nota da FINAL (0 a 100): ");
        scanf("%d", &notaFinal);

        while (notaFinal < 0  notaFinal > 100)
        {
            printf(" Nota invalida! Digite novamente a nota da FINAL: ");
            scanf("%d", &notaFinal);
        }


        if (notaFinal > 70)
        {
            printf("\n Parabens, voce passou na FINAL! Nota da FINAL = %d", notaFinal);
        }
        else
        {
            printf("\n Nao foi dessa vez! Nota da FINAL = %d", notaFinal);
        }
    }
    else 
    {
        printf("\n Reprovado direto! Nota = %d", soma);
    }


    return 0;
}